<Window x:Class="GeoLib.WindowsHost.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GeoLib.WindowsHost"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="400">
    <StackPanel>
        <Button Name="btnStart" Content="Start Services" HorizontalAlignment="Center" Margin="20" VerticalAlignment="Top" Width="200" Height="50" Click="BtnStart_Click"/>
        <Button Name="btnStop" Content="Stop Services" HorizontalAlignment="Center" VerticalAlignment="Top" Width="200" Height="50" Click="BtnStop_Click"/>
        <Label Name="lblMessage" HorizontalAlignment="Center" Margin="20"/>
    </StackPanel>
</Window>
